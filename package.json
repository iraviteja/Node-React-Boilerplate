{
  "name": "node-react-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate in node-react with redux, materialUi and saas.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iraviteja/Node-React-Boilerplate.git"
  },
  "keywords": [
    "Node",
    "React",
    "Boilerplate",
    "materialUi",
    "saas",
    "redux"
  ],
  "author": "Ravi Teja",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iraviteja/Node-React-Boilerplate/issues"
  },
  "homepage": "https://github.com/iraviteja/Node-React-Boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "mongoose": "^5.4.0"
  }
}
